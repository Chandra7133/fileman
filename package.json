{
 "name": "fileman",
 "version": "1.0.1",
 "description": "file manager",
 "main": "main.js",
 "author": {
  "name": "Chandra Sekhar",
  "email": "chandrasekhar@bijib.com"
 },
 "homepage": "https://github.com/Chandra7133/fileman",
 "build": {
  "appId": "com.chandra.fileman",
  "productName": "FileMan",
  "directories": {
   "output": "release"
  },
  "files": [
   "dist/",
   "main.js",
   "preload.js",
   "file-curd.js",
   "file-handler.js",
   "package.json",
   "public/"
  ],
  "linux": {
   "target": [
    "deb"
   ],
   "category": "Utility",
   "icon": "public/icon.png",
   "maintainer": "Chandra Sekhar <chandrasekhar@bijib.com>"
  }
 },
 "scripts": {
  "ng": "ng",
  "start": "ng serve",
  "build": "ng build",
  "electron": "ng build --base-href ./ && electron .",
  "electron:linux": "ng build --base-href ./ && electron-builder --linux deb",
  "watch": "ng build --watch --configuration development",
  "test": "ng test"
 },
 "prettier": {
  "overrides": [
   {
    "files": "*.html",
    "options": {
     "parser": "angular"
    }
   }
  ]
 },
 "private": true,
 "dependencies": {
  "@angular/common": "^20.0.0",
  "@angular/compiler": "^20.0.0",
  "@angular/core": "^20.0.0",
  "@angular/forms": "^20.0.0",
  "@angular/platform-browser": "^20.0.0",
  "@angular/router": "^20.0.0",
  "bootstrap": "^5.3.7",
  "rxjs": "~7.8.0",
  "tslib": "^2.3.0",
  "xlsx": "^0.18.5"
 },
 "devDependencies": {
  "@angular-devkit/build-angular": "^20.1.0",
  "@angular/build": "^20.0.4",
  "@angular/cli": "^20.0.4",
  "@angular/compiler-cli": "^20.0.0",
  "@types/jasmine": "~5.1.0",
  "electron": "^37.2.1",
  "electron-builder": "^26.0.12",
  "jasmine-core": "~5.7.0",
  "karma": "~6.4.0",
  "karma-chrome-launcher": "~3.2.0",
  "karma-coverage": "~2.2.0",
  "karma-jasmine": "~5.1.0",
  "karma-jasmine-html-reporter": "~2.1.0",
  "typescript": "~5.8.2"
 }
}